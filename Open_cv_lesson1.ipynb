   "metadata": {},
   "outputs": [],
   "source": [
    "import cv2 as cv \n",
    "\n",
    "img=cv.imread('test_image.jpg')\n",
    "\n",
    "small=cv.resize(img,(300,200))\n",
    "large=cv.resize(img,(1000,700))\n",
    "\n",
    "cv.imshow('Small_image',small)\n",
    "cv.imshow('Large',large)\n",
    "cv.waitKey(0)\n",
    "cv.destroyAllWindows()"
   ]
  }
